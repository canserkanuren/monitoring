        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es5",
        "lib": ["es5", "es6", "dom"],
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "types": ["node"]
